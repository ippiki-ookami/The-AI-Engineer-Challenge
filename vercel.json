{
    "functions": {
        "api/homework_app.py": {
            "runtime": "@vercel/python"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/homework_app.py"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/unified.html"
        }
    ]
} 